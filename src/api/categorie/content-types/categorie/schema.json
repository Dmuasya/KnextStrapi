{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "categorie",
    "pluralName": "categories",
    "displayName": "Categorie",
    "name": "categorie"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 20,
      "minLength": 3
    },
    "image_url": {
      "type": "text"
    },
    "posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::post.post",
      "inversedBy": "categories"
    },
    "views": {
      "type": "biginteger",
      "unique": false,
      "default": "0",
      "required": true
    }
  }
}
